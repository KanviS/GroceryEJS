<%- include('./parts/header') %>
<h1>Add Grocery List</h1>
<form method="post" action="/add">
        <div class="form-group">
            <label for="name">Name</label>
            <input type="text" class="form-control" id="name" placeholder="Enter name">
        </div>
        <div class="form-group">
            <label for="imgUrl">Image Url</label>
            <input type="text" class="form-control" id="imgUrl" placeholder="Enter imgUrl">
        </div>
        <div class="form-group">
            <label for="quantity">Quantity</label>
            <input type="text" class="form-control" id="quantity" placeholder="Enter quantity">
        </div>
        <div class="form-group">
            <label for="cost">Cost</label>
            <input type="text" class="form-control" id="cost" placeholder="Enter cost">
        </div>
        <button type="submit" class="btn btn-success" data-dismiss="modal">Add</button>
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
</form>
<%- include('./parts/tail') %>